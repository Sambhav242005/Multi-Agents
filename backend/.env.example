# API Keys
OPENAI_API_KEY=your_openai_api_key_here
# Optional: Custom OpenAI base URL (e.g., for Azure OpenAI, OpenRouter, or other proxy)
# Example for OpenRouter: https://openrouter.ai/api/v1
OPENAI_API_BASE=https://openrouter.ai/api/v1
# Model Configuration
# Set to 'true' to use the same model for all agents (recommended for simplicity)
# Set to 'false' to use different models per agent
USE_SINGLE_MODEL=true

# Default model (used when USE_SINGLE_MODEL=true or as fallback)
# Examples for OpenRouter:
# - z-ai/glm-4.5-air:free (fast, free)
# - google/gemini-2.0-flash-exp:free (Google's latest)
# - meta-llama/llama-3.3-70b-instruct (powerful)
DEFAULT_MODEL=z-ai/glm-4.5-air:free

# Per-Agent Models (only used when USE_SINGLE_MODEL=false)
# Uncomment and customize these if you want different models for different agents
# CLARIFIER_MODEL=google/gemini-2.0-flash-exp:free
# PRODUCT_MODEL=meta-llama/llama-3.3-70b-instruct
# CUSTOMER_MODEL=anthropic/claude-3.5-sonnet
# ENGINEER_MODEL=meta-llama/llama-3.3-70b-instruct
# RISK_MODEL=anthropic/claude-3.5-sonnet
# SUMMARIZER_MODEL=google/gemini-2.0-flash-exp:free
# PROMPT_GENERATOR_MODEL=z-ai/glm-4.5-air:free
# DIAGRAM_MODEL=z-ai/glm-4.5-air:free
# TTS_CONVERTER_MODEL=z-ai/glm-4.5-air:free

# Security
JWT_SECRET_KEY=your_secret_key_here_generate_with_openssl_rand_hex_32

USE_SINGLE_MODEL=true
DEFAULT_MODEL=z-ai/glm-4.5-air:free